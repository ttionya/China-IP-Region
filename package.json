{
  "name": "ip-region",
  "version": "1.1.0",
  "description": "Get China IP region.",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "cross-env NODE_PORT=8101 pm2 start ./src/index.js --name China-IP-Region",
    "restart": "pm2 restart China-IP-Region",
    "stop": "pm2 stop China-IP-Region",
    "lint": "eslint --fix --ext .ts,.js ."
  },
  "engines": {
    "node": ">=10.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttionya/China-IP-Region.git"
  },
  "keywords": [
    "IP",
    "Region",
    "China IP"
  ],
  "author": "ttionya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ttionya/China-IP-Region/issues"
  },
  "homepage": "https://github.com/ttionya/China-IP-Region",
  "dependencies": {
    "cross-env": "^7.0.0",
    "koa": "^2.11.0",
    "ts-node": "^8.6.0"
  },
  "devDependencies": {
    "@ttionya/eslint-config": "^1.0.0",
    "@types/koa": "^2.11.0",
    "@types/node": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^1.19.0",
    "typescript": "^3.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix --ext .ts,.js"
    ]
  }
}

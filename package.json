{
  "name": "ip-region",
  "version": "0.0.0-alpha0",
  "description": "Get China IP region.",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d ./dist",
    "start": "pm2 start ./dist/index.js --name China-IP-Region",
    "restart": "pm2 restart China-IP-Region",
    "stop": "pm2 stop China-IP-Region",
    "lint": "eslint . --fix"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttionya/China-IP-Region.git"
  },
  "keywords": [
    "IP",
    "Region",
    "China IP"
  ],
  "author": "ttionya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ttionya/China-IP-Region/issues"
  },
  "homepage": "https://github.com/ttionya/China-IP-Region",
  "dependencies": {
    "koa": "^2.7.0"
  },
  "devDependencies": {
    "eslint": "^5.0.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
